            "^@dish/(.*)$": "<rootDir>/dish/$1",
            "^@dish$": "<rootDir>/dish",
            "^@validators/(.*)$": "<rootDir>/validators/$1",
            "^@validators$": "<rootDir>/validators",
            "^@dto/(.*)$": "<rootDir>/dto/$1",
            "^@dto$": "<rootDir>/dto",
            "^@upload/(.*)$": "<rootDir>/upload/$1",
            "^@upload$": "<rootDir>/upload",
            "^@review/(.*)$": "<rootDir>/review/$1",
            "^@review$": "<rootDir>/review",
            "^@menu/(.*)$": "<rootDir>/menu/$1",
            "^@menu$": "<rootDir>/menu",
            "^@faq/(.*)$": "<rootDir>/faq/$1",
            "^@faq$": "<rootDir>/faq",
            "^@city/(.*)$": "<rootDir>/city/$1",
            "^@city$": "<rootDir>/city",
            "^@order/(.*)$": "<rootDir>/order/$1",
            "^@order$": "<rootDir>/order",
            "^@promocode/(.*)$": "<rootDir>/promocode/$1",
            "^@promocode$": "<rootDir>/promocode",
            "^@settings/(.*)$": "<rootDir>/settings/$1",
            "^@settings$": "<rootDir>/settings",
            "^@redis/(.*)$": "<rootDir>/redis/$1",
            "^@redis$": "<rootDir>/redis",
            "^@shared/(.*)$": "<rootDir>/shared/$1",
            "^@shared$": "<rootDir>/shared"
        }
    }
}
